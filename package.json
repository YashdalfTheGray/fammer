{
  "name": "fammer",
  "version": "1.0.0",
  "description": "Way to share food like people share other stuff",
  "main": "index.js",
  "scripts": {
    "start": "concurrently -n \"webpack,server\" -c \"cyan,green\" -s last \"npm run watch\" \"npm run dev\"",
    "dev": "cross-env NODE_ENV=development node index.js",
    "watch": "cross-env NODE_ENV=development echo \"webpack watching\"",
    "preprod": "cross-env NODE_ENV=production echo \"webpack production build\"",
    "prod": "cross-env NODE_ENV=production node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YashdalfTheGray/fammer.git"
  },
  "keywords": [
    "food",
    "vue",
    "material",
    "sharing",
    "node",
    "javascript"
  ],
  "author": "Yash Kulshrestha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YashdalfTheGray/fammer/issues"
  },
  "homepage": "https://github.com/YashdalfTheGray/fammer#readme",
  "dependencies": {
    "@std/esm": "^0.19.7",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "@std/esm": "cjs"
}
